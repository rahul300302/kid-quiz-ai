#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/officeparser@6.0.4/node_modules/officeparser/dist/node_modules:/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/officeparser@6.0.4/node_modules/officeparser/node_modules:/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/officeparser@6.0.4/node_modules:/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/officeparser@6.0.4/node_modules/officeparser/dist/node_modules:/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/officeparser@6.0.4/node_modules/officeparser/node_modules:/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/officeparser@6.0.4/node_modules:/mnt/d/RahulP/nextjs/kid-quiz-ai/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/officeparser@6.0.4/node_modules/officeparser/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/officeparser@6.0.4/node_modules/officeparser/dist/index.js" "$@"
fi
